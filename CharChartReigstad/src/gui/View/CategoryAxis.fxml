<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controllers.CategoryAxisController">
   <children>
      <Button layoutX="63.0" layoutY="690.0" mnemonicParsing="false" onAction="#DrawAreaChart" prefHeight="25.0" prefWidth="919.0" text="Draw Chart" />
      <Button fx:id="LineChartBt" layoutX="63.0" layoutY="747.0" mnemonicParsing="false" onAction="#LineChartGoBt" text="LineChart" />
      <Button fx:id="scatterChartArea" layoutX="155.0" layoutY="747.0" mnemonicParsing="false" onAction="#scatterChartBt" text="ScatterChart" />
      <Button fx:id="AreaChartbt" layoutX="260.0" layoutY="747.0" mnemonicParsing="false" onAction="#AreaChartBtCat" text="AreaChart" />
      <Button layoutX="356.0" layoutY="747.0" mnemonicParsing="false" text="Button" />
      <Text layoutX="455.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category Axis">
         <font>
            <Font size="29.0" />
         </font>
      </Text>
      <LineChart fx:id="categoryAxisChart" layoutX="53.0" layoutY="62.0" prefHeight="609.0" prefWidth="929.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
