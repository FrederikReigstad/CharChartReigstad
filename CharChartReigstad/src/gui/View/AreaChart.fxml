<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.chart.AreaChart?>
<AnchorPane prefHeight="800.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controllers.AreaChartController">
    <children>
        <AreaChart fx:id="chartLine" layoutX="35.0" layoutY="57.0" prefHeight="623.0" prefWidth="954.0">
            <xAxis>
                <NumberAxis side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT"/>
            </yAxis>
        </AreaChart>
        <Button layoutX="53.0" layoutY="680.0" mnemonicParsing="false" onAction="#DrawChart" prefHeight="25.0"
                prefWidth="919.0" text="Draw Chart"/>
        <Button fx:id="LineChartBt" layoutX="53.0" layoutY="737.0" mnemonicParsing="false" onAction="#LineChartGoBt"
                text="LineChart"/>
        <Button layoutX="145.0" layoutY="737.0" mnemonicParsing="false" text="Button"/>
        <Button layoutX="219.0" layoutY="737.0" mnemonicParsing="false" text="Button"/>
        <Button layoutX="291.0" layoutY="737.0" mnemonicParsing="false" text="Button"/>
    </children>
</AnchorPane>
