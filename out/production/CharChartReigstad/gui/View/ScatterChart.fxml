<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controllers.ScatterChartController">
   <children>
      <Button layoutX="63.0" layoutY="690.0" mnemonicParsing="false" onAction="#DrawAreaChart" prefHeight="25.0" prefWidth="919.0" text="Draw Chart" />
      <Button fx:id="LineChartBt" layoutX="63.0" layoutY="747.0" mnemonicParsing="false" onAction="#LineChartGoBt" text="LineChart" />
      <Button layoutX="284.0" layoutY="747.0" mnemonicParsing="false" text="Button" />
      <Button layoutX="356.0" layoutY="747.0" mnemonicParsing="false" text="Button" />
      <Button fx:id="AreaChartBt" layoutX="171.0" layoutY="747.0" mnemonicParsing="false" onAction="#AreaChartGoBt" text="AreaChart" />
      <Text layoutX="460.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Scatter Chart">
         <font>
            <Font size="28.0" />
         </font>
      </Text>
      <ScatterChart fx:id="ScatterChartCh" layoutX="53.0" layoutY="53.0" prefHeight="610.0" prefWidth="919.0">
         <xAxis>
            <CategoryAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </ScatterChart>
      <Button fx:id="CategoryAxisBt" layoutX="264.0" layoutY="747.0" mnemonicParsing="false" onAction="#CategoryAxisBt" text="CategoryAxis" />
   </children>
</AnchorPane>
